{
  "name": "panoramic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./bin/www",
    "debug": "DEBUG=pois:* babel-node ./bin/www",
    "build": "NODE_ENV=test npm test && rimraf dist/* && rimraf public/assets/* && babel -d dist/server server && babel -d dist/app app && NODE_ENV=production webpack --config webpack.config.prod",
    "test": "NODE_ENV=test mocha --colors --reporter dot --compilers js:babel-register --recursive test/",
    "test:cov": "NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha -- --colors --reporter dot --compilers js:babel-register --recursive test/",
    "test:ci": "NODE_ENV=test mocha --reporter spec --compilers js:babel-register --watch --recursive test/",
    "ci": "webpack-dev-server --inline --port 8081 --config webpack.config --output-public-path http://localhost:8081/assets --progress --content-base /public",
    "lint": "./node_modules/.bin/eslint app --ext .js,.jsx"
  },
  "pre-commit": [
    "test"
  ],
